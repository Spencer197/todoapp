<%= render 'shared/page_title', title: "#{@factor.name}" %>
<% if @factor.todos.count != 0 %>
  <h4 align="center">Todos Requiring <%= @factor.name.titleize %></h4>
  <div align="center">
    <%= will_paginate @factor_todos %>
  </div>
  <%= render 'todos/todo', obj: @factor_todos %>
  <div align="center">
    <%= will_paginate @factor_todos %>
  </div>
<% else %>
  <h4 align="center">No todos yet!</h4>
<% end %>